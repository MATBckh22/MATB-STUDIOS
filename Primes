def is_prime(num):
    if num <= 0:
        return False
    elif num == 3 or num == 5 or num == 7 or num == 11:
        return True
    elif num == 1:
        return True
    else:
        if num/2 == int or num/3 == int or num/5 == int or num/7 == int or num/11 == int:
            return False
        else: 
            return True