def sum_dig_pow(a, b): # range(a, b + 1) will be studied by the function
    results = []
    for i in range(a, b+1):
        sum = 0
        s = str(i)
        for counter in range(len(s)):
            sum = sum + int(s[counter])**(counter+1)
        if sum == i:
            results.append(i)
    return results